{
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 80,
  "include": [
    "**/*.js"
  ],
  "exclude": [
    "**/*.spec.js",
    "coverage",
    "*.js",
    "src/*.js"
  ],
  "reporter": [
    "html",
    "text",
    "text-summary"
  ],
  "require": [
    "babel-register",
    "babel-polyfill"
  ],
  "extension": [
    ".js"
  ],
  "cache": false,
  "all": false,
  "check-coverage": true
}
